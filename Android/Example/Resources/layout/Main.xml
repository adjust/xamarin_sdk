<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent">
    <Button
        android:id="@+id/btnTrackSimpleEvent"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="@string/event_simple" />
    <Button
        android:id="@+id/btnTrackRevenueEvent"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="@string/event_revenue" />
    <Button
        android:id="@+id/btnTrackCallbackEvent"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="@string/event_callback" />
    <Button
        android:id="@+id/btnTrackPartnerEvent"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="@string/event_partner" />
    <Button
        android:id="@+id/btnEnableOfflineMode"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="@string/offline_enable" />
    <Button
        android:id="@+id/btnDisableOfflineMode"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="@string/offline_disable" />
    <Button
        android:id="@+id/btnEnableSDK"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="@string/sdk_enable" />
    <Button
        android:id="@+id/btnDisableSDK"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="@string/sdk_disable" />
    <Button
        android:id="@+id/btnIsSDKEnabled"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="@string/sdk_is_enabled" />
    <Button
        android:text="@string/sdk_get_ids"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/btnGetIds" />
    <Button
        android:text="@string/gdpr_forget_me"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/btnGdprForgetMe" />
</LinearLayout>