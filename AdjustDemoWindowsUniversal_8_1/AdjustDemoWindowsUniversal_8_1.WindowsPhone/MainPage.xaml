<Page
    x:Class="AdjustDemoWindowsUniversal_8_1.MainPage"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:AdjustDemoWindowsUniversal_8_1"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d"
    Background="{ThemeResource ApplicationPageBackgroundThemeBrush}">

    <Grid>
        <Button x:Name="btnSimpleEvent" Content="Track simple event" HorizontalAlignment="Left" Margin="10,0,0,0" VerticalAlignment="Top" Click="btnSimpleEvent_Click"/>
        <Button x:Name="btnRevenueEvent" Content="Track revenue event" HorizontalAlignment="Left" Margin="10,40,0,0" VerticalAlignment="Top" Click="btnRevenueEvent_Click"/>
        <Button x:Name="btnCallbakEvent" Content="Track event with callback parameters" HorizontalAlignment="Left" Margin="10,80,0,0" VerticalAlignment="Top" Click="btnCallbakEvent_Click" />
        <Button x:Name="btnPartnerEvent" Content="Track event with partner parameters" HorizontalAlignment="Left" Margin="10,120,0,0" VerticalAlignment="Top" Click="btnPartnerEvent_Click" />
    </Grid>
</Page>